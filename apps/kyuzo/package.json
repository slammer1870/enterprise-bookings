{
  "name": "kyuzo",
  "version": "1.0.0",
  "description": "A blank template to get started with Payload 3.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
    "ci": "payload migrate:fresh --force-accept-warning && sleep 2 && pnpm build",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "migrate": "printf 'y\\n' | cross-env NODE_OPTIONS=--no-deprecation payload migrate --force-accept-warning",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "test:e2e": "cross-env NODE_OPTIONS=--no-deprecation pnpm exec playwright test --reporter=dot",
    "test:e2e:ci": "cross-env CI=1 PW_TEST_HTML_REPORT_OPEN=never NODE_OPTIONS=--no-deprecation pnpm exec playwright test --reporter=dot"
  },
  "dependencies": {
    "@daveyplate/better-auth-ui": "^3.2.11",
    "@hookform/resolvers": "*",
    "@mailchimp/mailchimp_marketing": "^3.0.80",
    "@next/third-parties": "^15.5.4",
    "@payloadcms/db-postgres": "*",
    "@payloadcms/email-resend": "*",
    "@payloadcms/next": "*",
    "@payloadcms/payload-cloud": "*",
    "@payloadcms/plugin-form-builder": "*",
    "@payloadcms/plugin-nested-docs": "^3.43.0",
    "@payloadcms/plugin-seo": "*",
    "@payloadcms/plugin-stripe": "*",
    "@payloadcms/richtext-lexical": "*",
    "@payloadcms/ui": "*",
    "better-auth": "^1.4.5",
    "payload-auth": "^1.7.0",
    "@repo/auth-next": "workspace:*",
    "@repo/better-auth-config": "workspace:*",
    "@repo/bookings-plugin": "workspace:*",
    "@repo/bookings-next": "workspace:*",
    "@repo/children": "workspace:*",
    "@repo/memberships": "workspace:*",
    "@repo/payments-plugin": "workspace:*",
    "@repo/payments-next": "workspace:*",
    "@repo/roles": "workspace:*",
    "@repo/shared-services": "workspace:*",
    "@repo/shared-types": "workspace:*",
    "@repo/shared-utils": "workspace:*",
    "@repo/trpc": "workspace:*",
    "@repo/ui": "workspace:*",
    "@repo/website": "workspace:*",
    "@tanstack/react-query": "*",
    "@trpc/client": "^11.7.1",
    "@trpc/server": "^11.7.1",
    "@trpc/tanstack-react-query": "^11.7.1",
    "@types/mailchimp__mailchimp_marketing": "^3.0.21",
    "cross-env": "^10.1.0",
    "graphql": "^16.12.0",
    "lucide-react": "0.554.0",
    "monaco-editor": "^0.52.2",
    "next": "*",
    "next-plausible": "*",
    "next-sitemap": "^4.2.3",
    "payload": "*",
    "payload-plugin-masquerade": "^1.4.1",
    "react": "*",
    "react-dom": "*",
