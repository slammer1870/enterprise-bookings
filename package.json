{
  "name": "enterprise-bookings",
  "devDependencies": {
    "prettier": "^3.6.2",
    "syncpack": "^13.0.0",
    "turbo": "^2.3.3",
    "typescript": "5.9.3"
  },
  "engines": {
    "node": ">=20"
  },
  "packageManager": "pnpm@10.12.1",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "ci": "turbo run ci",
    "db:migrate": "turbo run payload -- migrate",
    "dev": "turbo dev",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "generate:importmap": "turbo run generate:importmap",
    "generate:types": "turbo run generate:types",
    "lint": "turbo lint",
    "syncpack": "syncpack",
    "test": "turbo run test",
    "test:e2e": "turbo test:e2e",
    "test:e2e:ci": "turbo test:e2e:ci",
    "test:int": "turbo test:int",
    "ui": "pnpm --filter @repo/ui ui"
  },
  "pnpm": {
    "overrides": {
      "stripe": "17.7.0",
      "react-hook-form": "7.56.3",
      "react": "19.2.1",
      "react-dom": "19.2.1",
      "payload": "3.43.0",
      "@payloadcms/db-postgres": "3.43.0",
      "@payloadcms/email-resend": "3.43.0",
      "@payloadcms/next": "3.43.0",
      "@payloadcms/payload-cloud": "3.43.0",
      "@payloadcms/plugin-form-builder": "3.43.0",
      "@payloadcms/plugin-nested-docs": "3.43.0",
      "@payloadcms/plugin-seo": "3.43.0",
      "@payloadcms/plugin-stripe": "3.43.0",
      "@payloadcms/richtext-lexical": "3.43.0",
      "@payloadcms/ui": "3.43.0",
      "next": "15.4.8",
      "payload-auth": "1.3.3",
      "better-auth": "1.3.11"
    }
  }
}
