{
  "name": "e2e-kyuzo",
  "version": "1.0.0",
  "description": "A blank template to get started with Payload 3.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "e2e:build": "cross-env NODE_OPTIONS=\"--no-deprecation --max-old-space-size=8000\" next build",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "test:e2e": "cross-env NODE_OPTIONS=--no-deprecation payload migrate:fresh --force-accept-warning && pnpm exec playwright test",
    "test:e2e:ci": "cross-env NODE_OPTIONS=--no-deprecation pnpm exec playwright test",
    "test:int": "cross-env NODE_OPTIONS=--no-deprecation vitest run --config ./vitest.config.mts"
  },
  "dependencies": {
    "@hookform/resolvers": "4.1.0",
    "@payloadcms/db-postgres": "*",
    "@payloadcms/email-resend": "*",
    "@payloadcms/next": "*",
    "@payloadcms/payload-cloud": "*",
    "@payloadcms/plugin-form-builder": "*",
    "@payloadcms/plugin-nested-docs": "^3.43.0",
    "@payloadcms/plugin-seo": "*",
    "@payloadcms/plugin-stripe": "*",
    "@payloadcms/richtext-lexical": "*",
    "@payloadcms/ui": "*",
    "@repo/auth": "workspace:*",
    "@repo/bookings": "workspace:*",
    "@repo/memberships": "workspace:*",
    "@repo/payments": "workspace:*",
    "@repo/roles": "workspace:*",
    "@repo/shared-services": "workspace:*",
    "@repo/shared-types": "workspace:*",
    "@repo/shared-utils": "workspace:*",
    "@repo/testing-config": "workspace:*",
    "@repo/ui": "workspace:*",
    "@repo/website": "workspace:*",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.7",
    "graphql": "^16.10.0",
    "http-status": "^2.1.0",
    "kyuzo": "workspace:*",
    "lucide-react": "0.475.0",
    "next": "*",
    "next-plausible": "^3.12.4",
    "payload": "*",
    "react": "*",
    "react-hook-form": "*",
    "sharp": "0.34.2",
    "sonner": "2.0.1",
    "ts-essentials": "^10.1.1",
    "zod": "^4.0.15"
  },
  "devDependencies": {
    "@playwright/test": "^1.54.1",
    "@testing-library/react": "16.3.0",
    "@types/node": "^22.10.7",
    "@types/react": "*",
    "@types/react-dom": "19.1.2",
    "@vitejs/plugin-react": "4.5.2",
    "eslint": "^9.18.0",
    "eslint-config-next": "15.3.0",
    "jsdom": "26.1.0",
    "prettier": "^3.4.2",
    "typescript": "5.7.3",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.2.3"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0",
    "pnpm": "^9 || ^10"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp",
      "esbuild",
      "unrs-resolver"
    ]
  }
}
