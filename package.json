{
  "name": "enterprise-bookings",
  "devDependencies": {
    "prettier": "^3.4.2",
    "syncpack": "^13.0.0",
    "turbo": "^2.3.3",
    "typescript": "5.7.3"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "pnpm@8.15.6",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "generate:importmap": "turbo run generate:importmap",
    "generate:types": "turbo run generate:types",
    "lint": "turbo lint",
    "syncpack": "syncpack",
    "test": "turbo run test",
    "ui": "pnpm --filter @repo/ui ui"
  }
}
