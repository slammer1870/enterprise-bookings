{
  "name": "e2e-kyuzo",
  "version": "1.0.0",
  "description": "A blank template to get started with Payload 3.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "e2e:build": "cross-env NODE_OPTIONS=\"--no-deprecation --max-old-space-size=8000\" next build",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "test:e2e": "cross-env NODE_OPTIONS=--no-deprecation payload migrate:fresh --force-accept-warning && pnpm exec playwright test",
    "test:e2e:ci": "cross-env NODE_OPTIONS=--no-deprecation pnpm exec playwright test",
    "test:int": "cross-env NODE_OPTIONS=--no-deprecation vitest run --config ./vitest.config.mts"
  },
  "dependencies": {
    "@hookform/resolvers": "*",
    "@payloadcms/db-postgres": "*",
    "@payloadcms/email-resend": "*",
    "@payloadcms/next": "*",
    "@payloadcms/payload-cloud": "*",
    "@payloadcms/plugin-form-builder": "*",
    "@payloadcms/plugin-nested-docs": "^3.43.0",
    "@payloadcms/plugin-seo": "*",
    "@payloadcms/plugin-stripe": "*",
    "@payloadcms/richtext-lexical": "*",
    "@payloadcms/ui": "*",
    "@repo/auth-plugin": "workspace:*",
    "@repo/auth-next": "workspace:*",
    "@repo/bookings-plugin": "workspace:*",
    "@repo/bookings-next": "workspace:*",
    "@repo/memberships": "workspace:*",
    "@repo/payments-plugin": "workspace:*",
    "@repo/payments-next": "workspace:*",
    "@repo/roles": "workspace:*",
    "@repo/shared-services": "workspace:*",
    "@repo/shared-types": "workspace:*",
    "@repo/shared-utils": "workspace:*",
    "@repo/testing-config": "workspace:*",
    "@repo/ui": "workspace:*",
    "@repo/website": "workspace:*",
    "cross-env": "^10.1.0",
    "dotenv": "^17.2.3",
    "graphql": "^16.12.0",
    "http-status": "^2.1.0",
    "kyuzo": "workspace:*",
    "lucide-react": "0.554.0",
    "monaco-editor": "^0.52.2",
    "next": "*",
    "next-plausible": "*",
    "payload": "*",
    "react": "*",
    "react-hook-form": "*",
    "sharp": "0.34.5",
    "sonner": "*",
    "ts-essentials": "^10.1.1",
    "zod": "*"
  },
  "devDependencies": {
    "@playwright/test": "1.56.1",
    "@testing-library/react": "16.3.0",
    "@types/node": "^24.10.1",
    "@types/react": "*",
    "@types/react-dom": "19.2.3",
    "@vitejs/plugin-react": "5.1.1",
    "eslint": "^9.39.1",
    "eslint-config-next": "15.5.6",
    "jsdom": "27.2.0",
    "prettier": "^3.6.2",
    "typescript": "5.9.3",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "4.0.12"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0",
    "pnpm": "^9 || ^10"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp",
      "esbuild",
      "unrs-resolver"
    ]
  }
}
